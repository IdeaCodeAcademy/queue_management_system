<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record model="ir.module.category" id="ica_queue_management.module_ica_queue_management">
            <field name="name">Queue</field>
            <!--        <field name="sequence">7</field>-->
        </record>

        <record id="group_cashier" model="res.groups">
            <field name="name">Cashier</field>
            <field name="category_id" ref="ica_queue_management.module_ica_queue_management"/>
        </record>

        <record id="group_pharmacy" model="res.groups">
            <field name="name">Pharmacy</field>
            <field name="category_id" ref="ica_queue_management.module_ica_queue_management"/>
        </record>
        <record id="group_reception" model="res.groups">
            <field name="name">Reception</field>
            <field name="category_id" ref="ica_queue_management.module_ica_queue_management"/>
        </record>

        <record id="group_management" model="res.groups">
            <field name="name">Management</field>
            <field name="category_id" ref="ica_queue_management.module_ica_queue_management"/>
            <field name="implied_ids" eval="[(4, ref('ica_queue_management.group_cashier')),(4, ref('ica_queue_management.group_reception')),(4, ref('ica_queue_management.group_pharmacy'))]"/>
        </record>
    </data>
</odoo>
